{
  "name": "Veload",
  "description": "Bike at Home",
  "repository": "https://github.com/spencermize/Veload.git",
  "version": "0.1.0",
  "main": "veload.js",
  "devDependencies": {
    "browserify-shim": "^3.8.14",
    "nodemon": "*",
    "sass": "^1.16.1",
    "stylelint": "^9.10.1"
  },
  "scripts": {
    "js:watch": "watchify main.js -o public/js/bundle.js -v -d",
    "css:lint": "stylelint --config build/.stylelintrc --syntax scss \"scss/**/*.scss\" --cache --cache-location \"build/.stylelintcache/\"",
    "css:watch": "sass build/scss/main.scss public/css/style.css --watch",
    "css": "npm run css:lint && npm run css:watch",
    "app:watch": "nodemon app.js",
    "watch": "start npm run js:watch && start npm run css:watch && start npm run app:watch"
  },
  "dependencies": {
    "@ansur/leaflet-pulse-icon": "^0.1.1",
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "annyang": "^2.6.1",
    "ant-plus": "0.0.22",
    "axios": "*",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "browserify": "*",
    "connect-session-sequelize": "*",
    "conversions": "^1.5.2",
    "dsmorse-gridster": "^0.8.0",
    "easytimer.js": "^3.0.1",
    "express": "4.16.4",
    "express-hbs": "^2.0.1",
    "express-session": "*",
    "font-awesome-assets": "0.0.8",
    "geolib": "*",
    "gps-to-gpx": "^1.4.0",
    "gps-util": "*",
    "greenlock-express": "*",
    "install": "^0.12.2",
    "jquery": "^3.3.1",
    "leaflet": "*",
    "list.js": "^1.5.0",
    "moment": "*",
    "mysql2": "*",
    "npm": "^6.6.0",
    "numeral": "^2.0.6",
    "popper.js": "^1.14.6",
    "sequelize": "*",
    "strava-v3": "*",
    "svgfont2js": "^0.1.2",
    "yargs": "*"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {}
}
